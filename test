<script>
document.addEventListener('DOMContentLoaded', () => {
    // Select the source element
    const clickSource = document.querySelector('[cg_click-source]');
    
    // Add event listener to the source element
    clickSource.addEventListener('click', () => {
        // Select the target element
        const clickTarget = document.querySelector('[cg_click-target]');
        
        // Trigger a click on the target element
        if (clickTarget) {
            clickTarget.click();
        }
    });
});

</script>

