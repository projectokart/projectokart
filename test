<script>
window.addEventListener("DOMContentLoaded", () => {
  const { ScrollObserver, valueAtPercentage } = aat;

  if (window.innerWidth >= 992) {
    const cardsContainer = document.querySelector(
      "[cg_stacking-cards-wrapper]"
    );
    const cards = cardsContainer.querySelectorAll("[cg_stacking-card]");
    cardsContainer.style.setProperty("--cards-count", cards.length);
    cardsContainer.style.setProperty(
      "--card-height",
      `${cards[0].clientHeight}px`
    );
    Array.from(cards).forEach((card, index) => {
      const wrapperStyles = window.getComputedStyle(cardsContainer);
      const gap = parseFloat(wrapperStyles.gap);
      const offsetTop = gap;
      //   card.style.paddingTop = `${gap}px`;
      if (index === cards.length - 1) {
        return;
      }
      const toScale = 1 - (cards.length - 1 - index) * 0.1;
      const nextCard = cards[index + 1];
      const cardInner = card.querySelector("[cg_stacking-card-inner]");
      ScrollObserver.Element(nextCard, {
        offsetTop,
        offsetBottom: window.innerHeight - card.clientHeight,
      }).onScroll(({ percentageY }) => {
        cardInner.style.scale = valueAtPercentage({
          from: 1,
          to: toScale,
          percentage: percentageY,
        });
        cardInner.style.filter = `brightness(${valueAtPercentage({
          from: 1,
          to: 0.6,
          percentage: percentageY,
        })})`;
      });
    });
  }
});

</script>

