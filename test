<script>
window.addEventListener("DOMContentLoaded", () => {
  const sliderElements = document.querySelectorAll(
    ".customer-stories_swiper-cards-component"
  );
  sliderElements.forEach((element) => {
    const loopMode = true;
    let sliderDuration = 300;
    const swiperInstance = new Swiper(element.querySelector(".swiper"), {
      speed: sliderDuration,
      spaceBetween: 24,
      autoHeight: false,
      followFinger: true,
      freeMode: false,
      allowTouchMove: window.innerWidth < 992,
      slideToClickedSlide: false,
      rewind: false,
      mousewheel: {
        forceToAxis: true,
      },
      keyboard: {
        enabled: true,
        onlyInViewport: true,
      },
      breakpoints: {
        480: {
          slidesPerView: 1,
        },
        768: {
          slidesPerView: 1.5,
        },
        992: {
          slidesPerView: 2.5,
        },
        1440: {
          slidesPerView: 3,
        },
      },
      pagination: {},
      navigation: {
        nextEl: element.querySelector(".is-next"),
        prevEl: element.querySelector(".is-previous"),
        disabledClass: "is-disabled",
      },
      slideActiveClass: "is-active",
      slideDuplicateActiveClass: "is-active",
      on: {
        init: function () {
          const progressBar = element.querySelector(
            ".swiper-progress-indicator"
          );
          updateProgressBar(progressBar, this);
        },
        slideChange: function () {
          const progressBar = element.querySelector(
            ".swiper-progress-indicator"
          );
          updateProgressBar(progressBar, this);
        },
      },
    });
    function updateProgressBar(progressBar, swiper) {
      const progressPercentage =
        swiper.progress === -0 ? 10 : swiper.progress * 100;
      progressBar.style.width = `${progressPercentage}%`;
    }
  });
});

